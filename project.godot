; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Menu",
"language": "GDScript",
"path": "res://Scripts/Menu.gd"
}, {
"base": "Node",
"class": "Menu_Entry",
"language": "GDScript",
"path": "res://Scripts/Menu_Entry.gd"
}, {
"base": "Node",
"class": "Module",
"language": "GDScript",
"path": "res://Scripts/Module.gd"
} ]
_global_script_class_icons={
"Menu": "",
"Menu_Entry": "",
"Module": ""
}

[application]

config/name="HoneycombGUI"
run/main_scene="res://Scenes/MenuScene.tscn"
run/low_processor_mode=true
boot_splash/image="res://Art/splash-screen.png"
config/icon="res://Art/splash-screen.png"

[display]

window/size/width=240
window/size/height=240
window/size/borderless=true
window/size/always_on_top=true

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom_font="res://Fonts/ZeldaOracles.tres"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
